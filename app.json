{
  "expo": {
    "name": "SpendyFly",
    "slug": "spendy-fly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/ios-light.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sofronijev.spendyFly",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#EEF5F1"
      },
      "icon": "./app/assets/ios-light.png",
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.misurapps.spendyfly"
    },
    "web": {
      "favicon": "./app/assets/adaptive-icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6928b3f1-f181-4c14-a110-d718064f777c"
      }
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "image": "./app/assets/splash-icon.png",
          "backgroundColor": "#EEF5F1",
          "dark": {
            "backgroundColor": "#162A26"
          },
          "imageWidth": 220
        }
      ]
    ]
  }
}
